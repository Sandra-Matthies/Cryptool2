  <!--- - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -->
  <icon file="AES.png" />
  <relevantPlugins>
    <plugin name="AESVisualization" />
  </relevantPlugins>
</sample>